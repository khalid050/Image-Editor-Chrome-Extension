{
    "manifest_version": 2,
    "name": "code_extension",
    "version":"2.0",
    "content_scripts" : [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": ["jquery-3.4.1.min.js","content.js"]
        }
    ],
    "browser_action":{
        "default_icon": "meme.png",
        "default_popup": "index.html"
    },
    "permissions" : ["storage"]
}